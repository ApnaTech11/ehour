<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:property-placeholder location="file://${ehour.home}/conf/ehour.properties" />

	<bean id="wicketApplication" class="net.rrm.ehour.ui.EhourWebApplication">
		<property name="authenticationManager" ref="authenticationManager" />
		<property name="version" value="0.8.4"></property>
		<property name="wikiBaseUrl" value="http://wiki.ehour.nl/confluence/display/DOCU" />
	</bean>
</beans>
